;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CAPA;AAUA,QAAA,eAAA,EAAA,CACA;AAAA,UAAA,SAAA,EAAA,mBAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CAVA;AAaA,QAAA,KAAA,EAAA,CACA;AAAA,UAAA,SAAA,EAAA,aAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CAbA;AAgBA,QAAA,IAAA,EAAA,CACA;AAAA,UAAA,SAAA,EAAA,YAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA;AAhBA;AApBA,KAAA;AA2CA,GA1HA;AA2HA,EAAA,YA3HA,0BA2HA,CAEA,CA7HA;AA8HA,EAAA,OA9HA,qBA8HA,CACA,CA/HA;AAgIA,EAAA,OAhIA,qBAgIA;AACA,IAAA,QAAA,CAAA,aAAA,CAAA,MAAA,EAAA,YAAA,CAAA,OAAA,EAAA,iCAAA;AACA,GAlIA;AAmIA,EAAA,aAnIA,2BAmIA;AACA,IAAA,QAAA,CAAA,aAAA,CAAA,MAAA,EAAA,eAAA,CAAA,OAAA;AACA,GArIA;AAsIA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,IADA,EACA;AACA,WAAA,UAAA,GAAA,IAAA,CAAA,EAAA;;AACA,UAAA,IAAA,CAAA,EAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,SAAA;AACA;AACA,KAPA;AAQA,IAAA,OARA,qBAQA;AAAA;;AACA,UAAA,WAAA,GAAA;AACA,QAAA,QAAA,EAAA,KAAA,UAAA,CAAA,KADA;AAEA,QAAA,MAAA,EAAA,UAFA,CAEA;;AAFA,OAAA,CADA,CAKA;AACA;;AACA,UAAA,KAAA,UAAA,CAAA,KAAA,KAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,eAAA,KAAA;AACA;;AAAA,OAVA,CAWA;;AACA,UAAA,UAAA,CAAA,KAAA,UAAA,CAAA,KAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,UAAA,CAAA,KAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,WAAA;AACA,OAfA,CAiBA;AACA;;;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,IAAA,CAnBA,CAmBA;;AACA,WAAA,UAAA,CAAA,IAAA;AACA,WAAA,QAAA,GAAA,KAAA,EAEA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,SAAA,CAAA,EAAA;AACA,SAPA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SATA;AAUA,OAXA,EAWA,IAXA,CAHA;AAgBA,KA7CA;AA8CA,IAAA,UA9CA,sBA8CA,QA9CA,EA8CA;AAAA;;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,cAAA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,WAAA,GAAA;AACA,YAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,KADA;AAEA,YAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,QAFA;AAGA,YAAA,IAAA,EAAA,MAAA,CAAA,UAAA,CAAA,IAHA;AAIA,YAAA,MAAA,EAAA;AAJA,WAAA;AAMA,UAAA,QAAA,CAAA,WAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,YAAA,MAAA,CAAA,QAAA,CAAA;AACA,cAAA,OAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OADA;AAEA,cAAA,IAAA,EAAA;AAFA,aAAA;;AAIA,YAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA,WAPA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EADA,CAEA;AACA;AACA;AACA;AACA;AACA,WAdA;AAeA,SAtBA,MAsBA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,iBAAA,KAAA;AACA;AACA,OA3BA;AA4BA,KA5EA;AA6EA,IAAA,SA7EA,qBA6EA,UA7EA,EA6EA;AACA,WAAA,KAAA,CAAA,UAAA,EAAA,WAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,SAAA;AACA,KAhFA;;AAiFA;;;AAGA,IAAA,SApFA,qBAoFA,MApFA,EAoFA;AAAA;;AACA,UAAA,IAAA,GAAA,MAAA;AACA,WAAA,KAAA,GAAA,WAAA,CAAA,YAAA;AACA,QAAA,IAAA;;AACA,YAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,aAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,IAAA,+BAAA,IAAA;AACA;AACA,OATA,EASA,IATA,CAAA;AAWA,KAjGA;;AAkGA;;;AAGA,IAAA,cArGA,4BAqGA;AACA;AACA,WAAA,UAAA,CAAA,IAAA,GAAA,OAAA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,IAAA,CAHA,CAIA;;AACA,MAAA,aAAA,CAAA,KAAA,KAAA,CAAA;AACA;AA3GA;AAtIA,CAAA","sourcesContent":["<template>\n    <div id=\"signup\" v-title data-title=\"注册\">\n        <div class=\"signup-warp\">\n            <ul class=\"menu-tab\" >\n                <li  v-for=\"(item,index) in menuTab\" :key=\"item.id\" @click=\"changeTab(item)\" :class='[ currentNum == index + 1  ? \"current\" : \"\"]'>{{item.txt}}</li>\n            </ul>\n            <!-- 表单提交 start -->\n            <el-form :model=\"formSignup\" :rules=\"rules\" ref=\"ruleSignup\">\n                <el-form-item prop=\"userName\"> \n                    <label class=\"labelname username\">用户名</label>\n                    <el-input class=\"loginInput\" type=\"text\" v-model=\"formSignup.userName\" placeholder=\"请输入用户名\"></el-input>\n                </el-form-item>\n                <el-form-item prop=\"email\" class=\"formItem\">\n                    <label class=\"labelname email\">邮箱</label>\n                    <el-input class=\"loginInput\" type=\"text\" v-model=\"formSignup.email\" placeholder=\"请输入邮箱\"></el-input>\n                </el-form-item>\n                <el-form-item prop=\"password\" class=\"formItem\">\n                    <label class=\"labelname password\">密码</label>\n                        <el-input class=\"loginInput\" type=\"password\" v-model=\"formSignup.password\" show-password placeholder=\"请输入密码\"></el-input>\n                </el-form-item>\n                <el-form-item prop=\"passwordConfirm\" class=\"formItem\">\n                    <label class=\"labelname password\">确认密码</label>\n                        <el-input class=\"loginInput\" type=\"password\" v-model=\"formSignup.passwordConfirm\" show-password placeholder=\"请确认密码\"></el-input>\n                </el-form-item>\n                <el-form-item prop=\"phone\" class=\"formItem\">\n                    <label class=\"labelname phone\">手机号码</label>\n                        <el-input class=\"loginInput\" type=\"text\" v-model=\"formSignup.phone\" placeholder=\"请输入手机号码\"></el-input>\n                </el-form-item>\n                <el-form-item prop=\"code\" class=\"formItem\">\n                    <label class=\"labelname code\">验证码</label>\n                    <el-row type=\"flex\" :gutter=\"2\" justify=\"space-between\">\n                        <el-col :span=\"14\">\n                                <el-input class=\"loginInput\" type=\"text\" v-model=\"formSignup.code\" placeholder=\"请输入验证码\"></el-input>\n                        </el-col>\n                        <el-col :span=\"9\">\n                            <el-button type=\"success\" @click=\"getCode()\" :disabled=\"codeStatus.status\">{{codeStatus.text}}</el-button>\n                        </el-col>\n                    </el-row>\n                </el-form-item>\n                <el-form-item class=\"submit-btn\">\n                    <el-button class=\"btn primary\" size=\"medium\" type=\"primary\"  @click=\"submitForm('ruleSignup')\" :disabled=\"isSignUp\">注册</el-button>\n                    <el-button class=\"btn cancel\" size=\"medium\" type=\"info\" @click=\"resetForm('ruleSignup')\">取消</el-button>\n                </el-form-item>\n            </el-form>\n            <!-- 表单提交 end -->\n        </div>\n        <router-view />\n    </div>\n</template>\n<script>\nimport { getSms, Register } from '@/api/login'\nimport SignUpComponents from \"@/components/login/SignUpComponents\"\nimport { stripUsername, stripscript, stripEmail, stripPassword, stripPhone } from \"@/utils/validate\"\nexport default {\n    name: \"SignUp\",\n    components: {\n        SignUpComponents\n    },\n    data(){\n        /*\n        * 验证用户名\n        */\n        var validateUserName = (rule,value,callback)=>{\n            if(value == \"\"){\n                callback(new Error(\"请输入用户名\"))\n            }else if(stripUsername(value)){\n                callback(new Error(\"用户名为4-16位(字母，数字，_，-)\"))\n            }else{\n                callback()\n            }\n        }\n        /*\n        * 验证邮箱规则\n        */\n        var validateEmail = ( rule,value,callback )=>{\n            if( value == \"\" ){\n                callback(new Error(\"请输入邮箱\"))\n            }else if(stripEmail(value)){\n                callback(new Error(\"请输入正确的邮箱\"))\n            }else{\n                callback()\n            }\n        };\n        /*\n        * 验证密码\n        */\n        var validatePass = (rule,value,callback)=>{\n            if( value === \"\" ){\n                callback(new Error(\"请输入密码\"))\n            }else if(stripPassword(value)){\n                callback(new Error(\"密码为6-20位数字与字母的组合\"))\n            } else {\n                callback();\n            }\n        };\n        /*\n        * 验证密码进行确认，是否两次密码相同\n        */\n        var validatePassConfirm = (rule,value,callback)=>{\n            if (value === '') {\n                callback(new Error('请再次输入密码'));\n            } else if (value !== this.formSignup.password) {\n                callback(new Error('两次输入密码不一致!'));\n            } else {\n                callback();\n            }\n        };\n        /*\n        * 验证手机号码\n        */\n        var validatePhone = (rule,value,callback)=>{\n            if(value === \"\"){\n                callback(new Error(\"请输入手机号码\"))\n            }else if(stripPhone(value)){\n                callback(new Error(\"请输入正确的手机号码\"))\n            }else{\n                callback()\n            }\n        };\n        /*\n        * 验证验证码\n        */\n        var validateCode = (rule,value,callback)=>{\n            if( value === \"\" ){\n                callback(new Error(\"请输入验证码\"))\n            } else {\n                callback();\n            }\n        };\n        var countDown = ()=>{\n            console.log('111')\n        }\n        return {\n            currentNum: 2,\n            isSignUp: true,//提交按钮‘禁用’\n            timer: null,//倒计时\n            menuTab: [\n                {id: 1,txt: '登录',current: false},\n                {id: 2,txt: '注册',current: true}\n            ],\n            codeStatus: {\n                status: false,//获取验证码时按钮禁用，并显示‘倒计时’\n                text: '获取验证码',\n            },\n            formSignup: {\n                userName: \"\",\n                email: \"\",\n                password: \"\",\n                passwordConfirm: \"\",\n                phone: \"\",\n                code: \"\"\n            },\n            rules: {\n                userName: [\n                    { validator: validateUserName, trigger: 'blur' },\n                ],\n                email: [\n                    { validator: validateEmail, trigger: 'blur' }\n                ],\n                password: [\n                    { validator: validatePass, trigger: 'blur' }\n                ],\n                passwordConfirm: [\n                    { validator: validatePassConfirm, trigger: 'blur' }\n                ],\n                phone: [\n                    { validator: validatePhone, trigger: 'blur' }\n                ],\n                code: [\n                    { validator: validateCode, trigger: 'blur' }\n                ]\n            }    \n        };\n        \n\n    },\n    beforeCreate(){\n        \n    },\n    created(){\n    },\n    mounted(){\n        document.querySelector('body').setAttribute('style', 'background-color:cornflowerblue');\n    },\n    beforeDestroy(){\n        document.querySelector('body').removeAttribute('style')\n    },\n    methods: {\n        changeTab(item){\n            this.currentNum = item.id\n            if(item.id == 1){\n                console.log(\"跳转到登录页面\")\n                this.$router.push(\"./Login\")\n            }\n        },\n        getCode(){\n            let requestData = {\n                username: this.formSignup.email,\n                module: 'register'//传递模型，后台验证邮箱是否存在\n            };\n            //前端验证的步骤\n            //验证邮箱不能为空\n            if(this.formSignup.email === ''){\n                this.$message.error('邮箱不能为空！！')\n                return false\n            };\n            //验证提交时的邮箱格式\n            if(stripEmail(this.formSignup.email)){\n                console.log(this.formSignup.email)\n                this.$message.error('邮箱格式不正确！！')\n            }\n            \n            //请求接口\n            //后端返回结果携带验证（防止前端数据没有将邮箱传递到后台）\n            this.codeStatus.status = true  //更改‘获取验证码’的状态，不可点击\n            this.codeStatus.text = `正在发送中`;\n            this.isSignUp = false,\n            \n            //设置倒计时\n            setTimeout(() => {\n                getSms(requestData).then( response =>{\n                    console.log(response.data)\n                    this.$message({\n                        message: response.data.message,\n                        type: 'success'\n                    });\n                    this.countDown(10)\n                }).catch( error =>{\n                    console.log(error)\n                })\n            }, 1000);\n            \n        },\n        submitForm(ruleName){\n            this.$options.methods.clearCountDown();\n            this.$refs[ruleName].validate((valid) => {\n                if (valid) {\n                    let requestData = {\n                        username: this.formSignup.email,\n                        password: this.formSignup.password,\n                        code: this.formSignup.code,\n                        module: 'register'\n                    };\n                    Register(requestData).then(response => {\n                        console.log(response.data)\n                        this.$message({\n                            message: response.data.message,\n                            type: 'success'\n                        })\n                        this.$router.push({path: \"/login\"})\n                    }).catch(error =>{\n                        console.log(error)\n                        //失败时执行的代码\n                        // this.$message({\n                        //     message: error.data.message,\n                        //     type: 'error'\n                        // })\n                    })\n                } else {\n                    console.log('error submit!!');\n                    return false;\n                }\n            });\n        },\n        resetForm(formSignup){\n            this.$refs[formSignup].resetFields();\n            this.$router.push(\"./Login\")\n        },\n        /**\n         * 倒计时\n        */\n        countDown(number){\n            let time = number;\n            this.timer = setInterval(()=>{\n                time--;\n                if(time === 0){\n                    clearInterval(this.timer);\n                    this.codeStatus.text = `请重新发送`;\n                    this.codeStatus.status = false;\n                }else{\n                    this.codeStatus.text = `倒计时${time}s`;\n                }\n            },1000)\n            \n        },\n        /**\n         * 清除倒计时\n        */\n        clearCountDown(){\n            //还原验证码框内的内容\n            this.codeStatus.text = '获取验证码';\n            this.codeStatus.status = true;\n            //清除倒计时\n            clearInterval(this.timer)\n        }\n    }\n}\n</script>\n<style lang=\"scss\" scoped>\n    #signup{\n        width: 100%;\n        min-height: 100%;\n        // background-color: cornflowerblue;\n        }\n    .signup-warp{\n        width: 300px;\n        position: relative;\n        margin: 0 auto;\n        padding: 100px 35px 0;\n    }\n    .menu-tab{\n        text-align: center;\n        li{\n            display: inline-block;\n            width: 88px;\n            line-height: 36px;\n            font-size: 14px;\n            color: #fff;\n            border-radius: 3px;\n            cursor: pointer;\n        }\n        .current{\n            background-color: rgba(0, 0, 0, 0.4);\n        }\n    }\n    .loginInput .el-input__inner:focus{\n        border-color:sandybrown;\n    }\n    .formItem{\n        margin-top: 10px;\n    }\n    .submit-btn{\n        margin-top: 20px;\n    }\n    .btn{\n        width: 100%;\n    }\n    .btn.cancel{\n        margin-left: 0;\n        margin-top: 10px;\n    }\n    .labelname{\n        color: #fff;\n    }\n    .el-row {\n        &:last-child {\n            margin-bottom: 0;\n        }\n    }\n    .el-col {\n        border-radius: 4px;\n    }\n</style>"],"sourceRoot":"src/views"}]}